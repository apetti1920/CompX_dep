{
  "name": "compx",
  "productName": "compx",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder -mwl",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "Aidan Petti",
    "email": "apetti1920@aol.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apetti1920/CompX.git"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "compx"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/app/windows/mainWindow/renderer.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.54",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/d3": "^6.2.0",
    "@types/he": "^1.1.1",
    "@types/lodash": "^4.14.168",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/three": "^0.125.3",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^4.3.0",
    "electron": "11.2.1",
    "electron-reload": "^1.5.0",
    "electron-reloader": "^1.2.0",
    "eslint": "^7.18.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "fork-ts-checker-webpack-plugin": "^5.2.1",
    "node-loader": "^1.0.2",
    "react-devtools-electron": "^4.7.0",
    "style-loader": "^1.3.0",
    "ts-loader": "^8.0.14",
    "webpack-hot-client": "^4.1.3",
    "webpack-hot-middleware": "^2.25.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.0",
    "csv-parser": "^3.0.0",
    "d3": "^6.4.0",
    "electron-is-dev": "^1.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "fast-xml-parser": "^3.17.6",
    "lodash": "^4.17.20",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.9",
    "react-jsx-parser": "^1.28.1",
    "react-redux": "^7.2.2",
    "react-three-fiber": "^5.3.19",
    "three": "^0.126.0",
    "typescript": "^4.1.3",
    "uuid": "^8.3.2"
  }
}
