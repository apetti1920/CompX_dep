{
  "name": "compx",
  "productName": "CompX",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make-mac": "npx @electron-forge/cli make --platform darwin",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts .",
    "build-installer": "electron-builder"
  },
  "keywords": [],
  "author": {
    "name": "apetti1920",
    "email": "apetti1920@aol.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./assets/images/logo.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "compx"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/app/windows/mainWindow/renderer.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "build": {
    "appId": "com.inflow.compx",
    "productName": "CompX",
    "mac": {
      "target": [
        "dmg"
      ],
      "identity": null,
      "icon": "./assets/images/logo.ico",
      "category": "public.app-category.productivity"
    },
    "dmg": {
      "background": "./assets/images/dmg_background.tiff",
      "icon": "./assets/images/logo.ico",
      "iconSize": 150,
      "window": {
        "width": 600,
        "height": 600
      },
      "contents": [
        {
          "x": 425,
          "y": 75
        },
        {
          "x": 425,
          "y": 350,
          "type": "link",
          "path": "/Application"
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.53",
    "@electron-forge/maker-deb": "^6.0.0-beta.53",
    "@electron-forge/maker-rpm": "^6.0.0-beta.53",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.53",
    "@electron-forge/maker-zip": "^6.0.0-beta.53",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.53",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "copy-webpack-plugin": "^6.3.2",
    "css-loader": "^4.3.0",
    "electron": "10.1.1",
    "electron-builder": "^22.9.1",
    "eslint": "^7.8.1",
    "eslint-plugin-import": "^2.22.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^5.1.0",
    "node-loader": "^1.0.1",
    "react-devtools-electron": "^4.7.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@types/d3": "^6.2.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "^26.0.13",
    "@types/lodash": "^4.14.161",
    "@types/react": "^16.9.49",
    "@types/react-dnd": "^3.0.2",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/uuid": "^8.3.0",
    "csstype": "^3.0.3",
    "csv-parser": "^3.0.0",
    "d3": "^6.3.1",
    "electron-is-dev": "^1.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-util": "^0.14.2",
    "eslint-plugin-react": "^7.20.6",
    "express": "^4.17.1",
    "fast-xml-parser": "^3.17.6",
    "framework-utils": "^0.3.4",
    "he": "^1.2.0",
    "jest": "^26.6.3",
    "lodash": "^4.17.20",
    "react": "^16.13.1",
    "react-burger-menu": "^3.0.0",
    "react-dom": "^16.13.1",
    "react-feather": "^2.0.8",
    "react-jsx-parser": "^1.28.1",
    "react-redux": "^7.2.1",
    "react-xml-parser": "^1.1.8",
    "reduce-reducers": "^1.0.4",
    "redux": "^4.0.5",
    "socket.io": "^2.3.0",
    "uuid": "^8.3.1",
    "xml-js": "^1.6.11"
  }
}
